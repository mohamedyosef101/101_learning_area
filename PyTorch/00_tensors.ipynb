    {
      "cell_type": "code",
      "source": [
        "# matrix multiplication in tf\n",
        "matrix_square = tf.matmul(matrix, tf.transpose(matrix))\n",
        "print(f\"Original matrix: \\n{matrix}\\nSquared one: \\n{matrix_square}\")"
      ],
